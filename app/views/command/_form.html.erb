<%= bootstrap_form_tag  url: "/command/show_data" do |f| %>
  <%= f.text_field :username, label: "One-IT username" %>
  <%= f.text_field :password, label: "One-IT password", type: "password" %>

<%= f.select(:app_name) do %>
    <% @application.each do |l| -%>
      <%= content_tag(:option, l.name, value: l.name) %>
    <% end %>
<% end %>

  <%= f.text_field :application_version %>
  <%= f.text_field :command %>
  <%= f.text_field :target %>
  <%= f.select :revision_control, [['Git','Git']], :prompt => "Select One" %>
  <%= f.submit class: "btn btn-primary", value: "Submit" %> 
<% end %>
